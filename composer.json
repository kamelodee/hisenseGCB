{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "alexeevdv/react-smpp": "1.0.x-dev",
        "darkaonline/l5-swagger": "^8.3",
        "darkaonline/ripcord": "^0.1.8",
        "edomato/net-smpp-reactphp-client": "^0.1.0",
        "firehed/simplelogger": "^2.4",
        "fruitcake/laravel-cors": "^2.0",
        "glushkovds/php-smpp": "0.5",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.75",
        "laravel/passport": "^10.4",
        "laravel/sanctum": "^2.15",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.3",
        "maatwebsite/excel": "^3.1",
        "php-smpp/php-smpp": "^1.2",
        "phpxmlrpc/phpxmlrpc": "^4.8",
        "psr/simple-cache": "1.0",
        "react/http": "^1.6",
        "spatie/laravel-medialibrary": "^9.12",
        "spatie/laravel-permission": "^5.5",
        "tbondois/odoo-ripcord": "^1.7",
        "the_alex_mark/laravel-sms": "^1.0",
        "yajra/laravel-datatables-oracle": "^9.21"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.9",
        "laravel/sail": "^1.0.1",
        "laravel/ui": "^3.4",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
